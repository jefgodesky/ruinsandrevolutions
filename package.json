{
  "name": "users-jefgodesky-sites-ruinsandrevolutions",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "npm-run-all -p dev:sass dev:astro",
    "dev:astro": "astro dev",
    "dev:sass": "sass --watch src/styles/global.scss public/styles/main.css",
    "start": "astro dev",
    "build": "npm-run-all build:sass build:astro",
    "build:astro": "astro check && astro build",
    "build:sass": "sass src/styles/global.scss public/styles/main.css --no-source-map",
    "preview": "astro preview",
    "astro": "astro"
  },
  "dependencies": {
    "@astrojs/check": "^0.7.0",
    "@astrojs/mdx": "^3.1.0",
    "@astrojs/rss": "^4.0.6",
    "astro": "^4.10.1",
    "typescript": "^5.4.5"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "sass": "^1.77.5"
  }
}
